{
  "name": "flash-express",
  "version": "0.0.0",
  "private": true,
  "main": "lambda.js",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "coverage": "nyc npm run test",
    "build": "rm -rf build && tsc",
    "start": "npm run build && node --require dotenv/config ./build/local.js",
    "dev": "tsc-watch --project . --outDir ./build --onSuccess \"nodemon --require dotenv/config ./build/local.js\"",
    "debug": "npm run build && node --inspect-brk ./build/local.js",
    "init-claudia": "npm run build && npm run copy-package && claudia generate-serverless-express-proxy --source build --express-module index",
    "copy-package": "cp package.json ./build/package.json && cp claudia.json ./build/claudia.json && cp lambda.js ./build/lambda.js",
    "create-api": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --source build",
    "update-api": "npm run build && npm run copy-package && claudia update --source build --use-s3-bucket flash-express --no-optional-dependencies"
  },
  "dependencies": {
    "@okta/jwt-verifier": "^2.3.0",
    "aws-sdk": "^2.1054.0",
    "aws-serverless-express": "^3.4.0",
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^11.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.95.15",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "claudia": "^5.14.0",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
